<p id="notice"><%= notice %></p>

<h1>Tweet Deets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweet_deets.each do |tweet_deet| %>
      <tr>
        <td><%= link_to 'Show', tweet_deet %></td>
        <td><%= link_to 'Edit', edit_tweet_deet_path(tweet_deet) %></td>
        <td><%= link_to 'Destroy', tweet_deet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tweet Deet', new_tweet_deet_path %>
